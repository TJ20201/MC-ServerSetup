{
	"mcss_ver": "1.2.0",

	"servsettings": {
		"allow-flight": {"desc": "Allow flight when not in Creative", "default": "false", "options": ["true", "false"], "tags": []},
		"allow-nether": {"desc": "Allow access to the Nether dimension", "default": "true", "options": ["true", "false"], "tags": []},
		"broadcast-console-to-ops": {"desc": "Output Console to Operators", "default": "true", "options": ["true", "false"], "tags": []},
		"broadcast-rcon-to-ops": {"desc": "Output RCON Console to Operators", "default": "true", "options": ["true", "false"], "tags": []},
		"difficulty": {"desc": "Server difficulty", "default": "easy", "options": ["peaceful", "easy", "normal", "hard"], "tags": []},
		"enable-command-block": {"desc": "Enable command block usage", "default": "false", "options": ["true", "false"], "tags": []},
		"enable-query": {"desc": "Enables GameSpy4 protocol server listener", "default": "false", "options": ["true", "false"], "tags": []},
		"enable-rcon": {"desc": "Enables remote access to the server console", "default": "false", "options": ["true", "false"], "tags": ["Unsafe"]},
		"enable-status": {"desc": "Makes the server appear as online on the server list", "default": "true", "options": ["true", "false"], "tags": []},
		"enforce-secure-profile": {"desc": "Force players to have a Mojang-signed public key to join", "default": "false", "options": ["true", "false"], "tags": []},
		"enforce-whitelist": {"desc": "Enforces the whitelist on the server", "default": "false", "options": ["true", "false"], "tags": []},
		"entity-broadcast-range-percentage": {"desc": "How many blocks close to the player entities must be to show", "default": "100", "options": ["*range-100"], "tags": []},
		"force-gamemode": {"desc": "Force players to switch to the default gamemode on join", "default": "false", "options": ["true", "false"], "tags": []},
		"function-permission-level": {"desc": "Permission level required to use the function command", "default": "2", "options": ["*range-4"], "tags": []},
		"gamemode": {"desc": "The default gamemode for the server", "default": "survival", "options": ["survival", "adventure", "spectator", "creative"], "tags": []},
		"generate-structures": {"desc": "Whether structures should be generated or not", "default": "true", "options": ["true", "false"], "tags": []},
		"hardcore": {"desc": "Force hard difficulty and restrict players who die to Spectator mode", "default": "false", "options": ["true", "false"], "tags": []},
		"hide-online-players": {"desc": "Hide player count on Multiplayer menu", "default": "false", "options": ["true", "false"], "tags": []},
		"level-name": {"desc": "The name of the main server world", "default": "world", "options": ["*string"], "tags": []},
		"level-seed": {"desc": "The seed for generating server worlds", "default": "", "options": ["*string"], "tags": []},
		"level-type": {"desc": "The generation preset to use for the main server world", "default": "minecraft\\:normal", "options": ["minecraft:normal", "minecraft:flat", "minecraft:large_biomes", "minecraft:amplified"], "tags": []},
		"max-players": {"desc": "Max amount of players who can connect to your server", "default": "20", "options": ["*range-200"], "tags": []},
		"max-world-size": {"desc": "Maximum blocks a world can be in width and length", "default": "29999984", "options": ["*range-29999984"], "tags": []},
		"motd": {"desc": "The message to appear under your server when not using Direct Connect", "default": "A Minecraft Server", "options": ["*string"], "tags": []},
		"online-mode": {"desc": "Whether or not a player needs a licensed copy of Minecraft to join", "default": "true", "options": ["true", "false"], "tags": ["Recommended: true"]},
		"op-permission-level": {"desc": "Permission level for Operators", "default": "4", "options": ["*range-4"], "tags": []},
		"player-idle-timeout": {"desc": "Amount of minutes a player can be idle for without being kicked", "default": "0", "options": ["*string"], "tags": []},
		"pvp": {"desc": "Whether or not players can kill and damage eachother", "default": "true", "options": ["true", "false"], "tags": []},
		"query.port": {"desc": "Port to use on the query server", "default": "25565", "options": ["*range-65536"], "tags": []},
		"rate-limit": {"desc": "Maximum amount of packets allowed to be sent by a player in one go before being kicked", "default": "0", "options": ["*string"], "tags": []},
		"rcon.password": {"desc": "Password to use for the RCON Console", "default": "", "options": ["*string"], "tags": []},
		"rcon.port": {"desc": "Port to use for the RCON Console", "default": "25575", "options": ["*range-65536"], "tags": []},
		"require-resource-pack": {"desc": "Whether or not the server Resource Pack is required to join", "default": "false", "options": ["true", "false"], "tags": []},
		"resource-pack": {"desc": "A URL to the server Resource Pack (leave blank to skip)", "default": "", "options": ["*string"], "tags": []},
		"resource-pack-prompt": {"desc": "Custom message to s end when require-resource-pack is enabled (leave blank to skip)", "default": "", "options": ["*string"], "tags": []},
		"resource-pack-sha1": {"desc": "SHA1 Hash to use to verify the server resource pack (leave blank to skip)", "default": "", "options": ["*string"], "tags": []},
		"server-ip": {"desc": "IP to use for connecting to the server (leave blank to use your Public IP)", "default": "", "options": ["*string"], "tags": ["Recommended: blank/localhost"]},
		"server-port": {"desc": "Port to use for connecting to the server", "default": "25565", "options": ["*range-65536"], "tags": []},
		"simulation-distance": {"desc": "Maximum distance for players to render entities", "default": "10", "options": ["*range-32"], "tags": []},
		"spawn-animals": {"desc": "Whether or not passives and neutral mobs should spawn", "default": "true", "options": ["true", "false"], "tags": []},
		"spawn-monsters": {"desc": "Whether or not hostile mobs should spawn", "default": "true", "options": ["true", "false"], "tags": []},
		"spawn-npcs": {"desc": "Whether or not villagers should spawn", "default": "true", "options": ["true", "false"], "tags": []},
		"spawn-protection": {"desc": "Size of the default spawn protection (calculated with 2x+1, x being the value here)", "default": "16", "options": ["*range-32"], "tags": []},
		"sync-chunk-writes": {"desc": "Enable writing multiple chunks at once", "default": "true", "options": ["true", "false"], "tags": []},
		"use-native-transport": {"desc": "Optimise packet sending on Linux", "default": "true", "options": ["true", "false"], "tags": []},
		"view-distance": {"desc": "Similar to simulation-distance, but renders chunks instead.", "default": "10", "options": ["*range-32"]}
	},

